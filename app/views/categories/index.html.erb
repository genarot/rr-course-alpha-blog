<h1 align="center">Listing all Categories</h1>
<ul class="listing">
  <% @categories.each do |category| %>
    <div class="row">
      <div class="well col-md-4 col-md-offset-4">
        <li class="article-title">
          <%= link_to "#{category.name}", category_path(category) %>
        </li>
      </div>
    </div>
  <% end %>
</ul>