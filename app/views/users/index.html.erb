<h2 class="text-center mt-4">Alpha Bloggers</h2>
<div class="row justify-content-center">
  <div class="flickr_pagination mb-4">
    <%= will_paginate @users, container: false %>
  </div>
  <div class="col-9 mt-4">
    <% @users.each do |user| %>
      <%= render 'user_card', user: user %>
    <% end %>
  </div>
  <div class="flickr_pagination mb-4">
    <%= will_paginate @users, container: false %>
  </div>
</div>